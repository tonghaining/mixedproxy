PTX CoRW
"Coherence, Read-Write"
{
0:%r3=0;
1:%r1=0;
x=0;
}
 P0@cta 0,gpu 0         | P1@cta 1,gpu 0         ;
 st.release.sys x, 1    | ld.acquire.sys %r1, x  ;
 ld.weak %blank1, x     | st.weak x, 2           ;
 ld.weak %blank2, x     | ld.acquire.sys %r3, x  ;
~exists
(P1:%r1 == 1 /\ P0:%r3 == 1)